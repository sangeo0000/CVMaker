<?xml version='1.0' encoding='utf-8'?>
<widget id="aom.cvmaker" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="PDFService">
        <param name="android-package" value="com.pdf.generator.PDFGenerator" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Printer">
        <param name="android-package" value="de.appplant.cordova.plugin.printer.Printer" />
    </feature>
    <feature name="ExitApp">
        <param name="android-package" value="cordova.custom.plugins.exitapp.ExitApp" />
    </feature>
    <name>cvmaker</name>
    <description>
        an application which makes a curriculam vitae (cv).
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/my_logo.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
